{
  "name": "restaurant-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "generate-client": "openapi-generator-cli generate -i ./src/client/schema.yaml -g typescript-fetch -o src/client/openapi --additional-properties=ngVersion=6.1.7,npmName=restClient,supportsES6=true,npmVersion=6.9.0,withInterfaces=true"
  },
  "dependencies": {
    "@mantine/core": "^8.2.5",
    "@mantine/form": "^8.2.8",
    "@mantine/hooks": "^8.2.5",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@t3-oss/env-core": "^0.12.0",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.2.2",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.131.5",
    "@tanstack/react-store": "^0.7.0",
    "@tanstack/router-plugin": "^1.121.2",
    "@tanstack/store": "^0.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.6",
    "zod": "^3.24.2"
